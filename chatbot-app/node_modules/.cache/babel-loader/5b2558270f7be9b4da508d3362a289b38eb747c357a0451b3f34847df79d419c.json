{"ast":null,"code":"import Header from \"./HeaderComponent.vue\";\nimport AIComponent from \"./AIComponent.vue\";\nimport ProductList from \"./ProductList.vue\";\nimport axios from \"axios\";\nexport default {\n  components: {\n    Header,\n    AIComponent,\n    ProductList\n  },\n  data() {\n    return {\n      products: []\n    };\n  },\n  methods: {\n    async fetchProducts() {\n      console.log(\"fetchProducts() is being called...\");\n      try {\n        //const response = await axios.get(`http://localhost:8000/api/products/?search=${query}`);\n        const response = await axios.get(`http://127.0.0.1:8000/show/`);\n        //query = \"\";\n        this.products = response.data;\n        console.log(\"Fetched Products:\", response.data); // Debugging\n        console.log(\"Hi\");\n      } catch (error) {\n        console.error(\"Error fetching products:\", error);\n      }\n    },\n    updateProductList(products) {\n      // Access the ProductList component using a ref\n      this.$refs.productList.updateProducts(products);\n    }\n  },\n  mounted() {\n    // Access the ProductList component using a ref\n    this.$nextTick(() => {\n      this.$refs.productList = this.$refs.productListComponent;\n    });\n  },\n  created() {\n    this.fetchProducts();\n  }\n};","map":{"version":3,"names":["Header","AIComponent","ProductList","axios","components","data","products","methods","fetchProducts","console","log","response","get","error","updateProductList","$refs","productList","updateProducts","mounted","$nextTick","productListComponent","created"],"sources":["C:\\Users\\sigmaa\\Desktop\\E-Commerce\\E-commerce-Website\\chatbot-app\\src\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div id = \"app\">\r\n    <Header />\r\n    <AIComponent @products-received=\"updateProductList\"/>\r\n    <ProductList :products=\"products\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Header from \"./HeaderComponent.vue\";\r\nimport AIComponent from \"./AIComponent.vue\